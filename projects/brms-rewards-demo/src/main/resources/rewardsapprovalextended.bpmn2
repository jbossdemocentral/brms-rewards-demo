        </assignment>
      </dataInputAssociation>
      <dataInputAssociation>
        <targetRef>_10_TaskNameInput</targetRef>
        <assignment>
          <from xsi:type="tFormalExpression">Log</from>
          <to xsi:type="tFormalExpression">_10_TaskNameInput</to>
        </assignment>
      </dataInputAssociation>
