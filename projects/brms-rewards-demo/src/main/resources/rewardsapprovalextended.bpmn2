        </assignment>
      </dataInputAssociation>
      <dataInputAssociation>
        <targetRef>_11_TaskNameInput</targetRef>
        <assignment>
          <from xsi:type="tFormalExpression">Log</from>
          <to xsi:type="tFormalExpression">_11_TaskNameInput</to>
        </assignment>
